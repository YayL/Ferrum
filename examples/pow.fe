fn main (argc: i32, argv: Vec) -> i32 {
    let num: i32 = pow(argc * 2 + 3, 4)

    return num
}

fn pow(base: i32, pow: i32) -> i64 {

    if (base == 0) {
        return 0
    } else if (pow == 0) {
        return 1
    }
    
    let res: i64 = base, i : i32 = 0

    while (++i < pow) {
        res *= base
    }

    return res
}
